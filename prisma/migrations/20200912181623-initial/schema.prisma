// 1
datasource db {
  provider = "sqlite" 
  url = "***"
}

// 2
generator client {
  provider = "prisma-client-js"
}

// 3
model Investor {
  id          Int      @id @default(autoincrement())
  investorName String
  commercial Boolean
   industrial Boolean
   retail Boolean
   hotel Boolean
   contacts  Contact[]
   wc String
   gau String
   kzn String
   allregions String
}

model Contact {
   id Int      @id @default(autoincrement())
  name String
  officeNo String
  mobileNo String
  email String
  investorName Investor @relation(fields: [investorID], references: [id]) 
  investorID Int
}